namespace lb
{
    class Program
    {
        static void Main(string[] args)
        {
            int x = Convert.ToInt32(Console.ReadLine());
            int y = 0;
            while  (x > 0)
            {
                y = y * 10 + x % 10;
                x = x / 10;
              
            }

                Console.WriteLine(y);
            
          
    
        }
    }
}
