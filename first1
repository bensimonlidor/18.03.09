namespace lb
{
    class Program
    {
        static void Main(string[] args)
        {
            int a, b;
            a = Convert.ToInt32(Console.ReadLine());

            b = Convert.ToInt32(Console.ReadLine());

            if (a > b)
            {
                Console.WriteLine(a);
            }
            else
            {
                Console.WriteLine(b);
            }
             
        }
    }
}


קלוט מספר מ1-10 ובדוק מי מהמספרים זוגי או אי זוגי


namespace lb
{
    class Program
    {
        static void Main(string[] args)
        {
            int i = Convert.ToInt32(Console.ReadLine());

            while (i > 10)
            {

                Console.WriteLine(i);
            }

                if (i % 2 == 0)
                {
                    Console.WriteLine("zugi");
                }
                else
                {
                    Console.WriteLine("e-zugi");

                }
            
        }
    }
}





